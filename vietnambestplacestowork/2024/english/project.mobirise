          "_name": "features13",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features12\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Button\" name=\"showButton\" checked>\n        <input type=\"range\" inline title=\"Items\" name=\"items\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Icon Size\" name=\"iconSize\" min=\"1\" max=\"4\" step=\"1\" value=\"2\">        \n        <input type=\"color\" name=\"iconColor\" title=\"Color\" value=\"#6592e6\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/bg2-9.jpg\" selected>\n            <input type=\"color\" title=\"Color\" value=\"#020f26\">\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">        \n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-12 schedule-header\">\n                <div class=\"item mbr-flex item-heading\">\n                    <div class=\"icon-box\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"29\" height=\"29\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#000000\" stroke-width=\"1.5\" stroke-linecap=\"round\" stroke-linejoin=\"round\"><rect x=\"3\" y=\"4\" width=\"18\" height=\"18\" rx=\"2\" ry=\"2\"></rect><line x1=\"16\" y1=\"2\" x2=\"16\" y2=\"6\"></line><line x1=\"8\" y1=\"2\" x2=\"8\" y2=\"6\"></line><line x1=\"3\" y1=\"10\" x2=\"21\" y2=\"10\"></line></svg>\n                    </div>\n                    <div class=\"text-box\">\n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5 date-schedule\" data-app-selector=\".icon-text\">\n                            AFTERNOON SESSION</h5>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-md-12 col-lg-12 schedule-wrapper\">\n                <div class=\"item mbr-flex item-border align-items-center\">\n                    <div class=\"time-wrapper\">                                          \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">12:00 – 13:00</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>SUMMIT &amp; CEREMONY: REGISTRATION &amp; NETWORKING​</b></h5>\n                    </div>\n                </div>\n                <div class=\"item mbr-flex item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">13:00 – 13:15</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>OPENING SESSION​</b></h5> \n                    </div>\n                </div>\n                <div class=\"item mbr-flex item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">13:15 – 14:00</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><span class=\"small-text\">Xplore - ANPHABE’S EXCLUSIVE REPORT:</span>​<br><b>THE X FACTORS</b></h5> \n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Speakers</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/thanh-nguyen.jpg\" alt=\"Thanh Nguyễn\">\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Ms.&nbsp;<b style>THANH NGUYEN</b><br><i>CEO &amp; Chief Happiness Officer<br>Anphabe</i></h4>\n                                    </div>\n                                </li>\n                                 <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/vanluu-thumb.jpg\" alt=\"Vân Lưu\">\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Ms.&nbsp;<b>LUU BAO VAN</b><br><i>Associate Director<br>Intage Vietnam LLC</i></h4>\n                                    </div>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">14:00 – 14:45</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <span class=\"small-text\">Xpert:</span><br><b>ONTO THE BANI WORLD</b></h5> \n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4\" mbr-theme-style=\"display-7\"><font color=\"#aeaeae\"><i>To be update</i></font></h4>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex item-border\">\n                    <div class=\"time-wrapper\">                        \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">14:45 – 15:45</div>\n                    </div>\n                     <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <b>AWARDS CEREMONY VIETNAM BEST PLACES TO WORK&nbsp;<sup>®</sup> 2024</b><br>MEDIUM ENTERPRISES Category</h5> \n                    </div>\n                </div>\n                \n                <div class=\"item mbr-flex item-border\">\n                    <div class=\"time-wrapper\">                        \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">15:45 – 16:05</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>TEA BREAK &amp; NETWORKING</b></h5>                        \n                    </div>\n                </div>                \n\n                <div class=\"item mbr-flex item-border\">\n                    <div class=\"time-wrapper\">        \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">16:05 – 16:15</div>\n                    </div>                    \n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <span class=\"small-text\">Xpress</span><br><b>BUILDING EMPLOYEES FINANCIAL AGILITY</b></h5>                         \n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Speaker</p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/willross.jpg\" alt=\"WILL ROSS\">\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">Mr.&nbsp;<b>WILL ROSS</b><br><i>Chief Marketing &amp; Distribution Officer (CMDO)<br>Dragon Capital</i></h4>\n                                    </div>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex item-border\">\n                    <div class=\"time-wrapper\">                        \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">16:15 – 16:35</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <span class=\"small-text\">Xchange</span><br><b>THE NEXT STEP&nbsp;</b><b style=\"font-size: 1.4rem; letter-spacing: 1.1px;\">IN COMPETENCY EVOLUTION</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4\" mbr-theme-style=\"display-7\"><font color=\"#aeaeae\"><i>To be update</i></font></h4>\n                    </div>\n                </div> \n\n            