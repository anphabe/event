          "_name": "features13",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features12\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Button\" name=\"showButton\" checked>\n        <input type=\"range\" inline title=\"Items\" name=\"items\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Icon Size\" name=\"iconSize\" min=\"1\" max=\"4\" step=\"1\" value=\"2\">        \n        <input type=\"color\" name=\"iconColor\" title=\"Color\" value=\"#6592e6\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/bg-2.png\">\n            <input type=\"color\" title=\"Color\" value=\"#fafafa\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">        \n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-12 schedule-header\">\n                <div class=\"item mbr-flex item-heading\">\n                    <div class=\"icon-box\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"29\" height=\"29\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#000000\" stroke-width=\"1.5\" stroke-linecap=\"round\" stroke-linejoin=\"round\"><rect x=\"3\" y=\"4\" width=\"18\" height=\"18\" rx=\"2\" ry=\"2\"></rect><line x1=\"16\" y1=\"2\" x2=\"16\" y2=\"6\"></line><line x1=\"8\" y1=\"2\" x2=\"8\" y2=\"6\"></line><line x1=\"3\" y1=\"10\" x2=\"21\" y2=\"10\"></line></svg>\n                    </div>\n                    <div class=\"text-box\">\n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5 date-schedule\" data-app-selector=\".icon-text\">\n                            <b>Ngày 23.11.2023</b> – HỘI NGHỊ NƠI LÀM VIỆC TỐT NHẤT VIỆT NAM 2023®</h5>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-md-12 col-lg-12 schedule-wrapper\">\n                <div class=\"item mbr-flex item-border align-items-center\">\n                    <div class=\"time-wrapper\">                                          \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">9:00</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Phiên sáng - Đón khách &amp; Networking</b></h5>\n                    </div>\n                </div>\n                <div class=\"item mbr-flex item-border align-items-center\">\n                    <div class=\"time-wrapper\">                                          \n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">9:10</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                            <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>TimeLeap: ĐỐI THOẠI VỚI TƯƠNG LAI</b></h5>\n                            <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                                Trả lời nhưng câu hỏi khó của NGÀY MAI để thực thi và chuẩn bị từ HÔM NAY. Lần đầu tiên trên sân khấu, sẽ có một màn đối đáp thú vị giữa 2 CEO danh tiếng và 1 nhân vật giả tưởng mang tên “Tương Lai’.\n                            </h4>\n                            <div class=\"schedule-speakers mb-4\">\n                                <div class=\"speaker-title\">\n                                    <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                        <b>Diễn giả</b></p>\n                                </div>\n                                <ul class=\"speaker-list\">\n                                    <li>\n                                        <div class=\"speaker-img\">\n                                            <img src=\"@PROJECT_PATH@/assets/images/mr.-binu-jacob-tng-gim-c-nestl-vit-nam.jpeg\" alt=\"Binu Jacob\">\n                                        </div>\n                                        <div class=\"speaker-detail\">\n                                            <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                                Ông <b>BINU JACOB</b><br><i>Tổng Giám Đốc<br> Nestlé Việt Nam</i></h4>\n                                        </div>\n                                    </li>\n                                    <li>\n                                        <div class=\"speaker-img\">\n                                            <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt=\"Lê Trí Thông\">\n                                        </div>\n                                        <div class=\"speaker-detail\">\n                                            <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                                Ông <b>TURAN EMIN</b><br><i>Tổng Giám Đốc<br>Sanofi Việt Nam</i></h4>\n                                        </div>\n                                    </li>\n                                </ul>\n                            </div>\n                            <div class=\"speaker-title\">\n                                    <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                        <b>Moderator</b></p>\n                                </div>\n                            <ul class=\"speaker-list\">\n                                    <li>\n                                        <div class=\"speaker-img\">\n                                            <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt=\"Binu Jacob\">\n                                        </div>\n                                        <div class=\"speaker-detail\">\n                                            <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                                Ông <b>HẢO TRẦN</b><br><i>Đồng Sáng Lập &amp; Giám Đốc Điều Hành<br>Vietcetera Media</i></h4>\n                                        </div>\n                                    </li>\n                              </ul>\n                        </div>\n                </div>\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">9:40</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>TimeFreeze: CHRO – NHỮNG NĂNG LỰC MỚI</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                            Đặc sản của sự kiện Anphabe với các chia sẻ súc tích nhưng đầy cảm hứng từ các chuyên gia hàng đầu.\n                        </h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/maiphuong-thumb-1.jpg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">Bà <b>TRỊNH MAI PHƯƠNG</b><br><i>Phó Chủ Tịch Phụ Trách Nhân Sự Unilever Việt Nam\n</i><div><i>Kiêm Nhân Sự Ngành Hàng Chăm sóc Cá nhân, Unilever Đông Nam Á</i></div></h4>\n                                    </div>\n                                </li>\n                      \n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">9:55</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>HR Multiverse: ĐA VŨ TRỤ HR - TÂM TÌNH MUỐN NÓI</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                            Thông qua những câu ‘hỏi xoáy – đáp xoay’ từ chính nhân viên, hãy cùng lắng nghe những tư vấn thông minh cùng những tâm sự chân thành từ 4 HRD giàu kinh nghiệm để có thêm những bài học về quản lý và phát triển con người.\n                        </h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list pd-b\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            <b>Đang cập nhật</b><br><i><br></i></h4>\n                                    </div>\n                                </li>\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            <b>Đang cập nhật</b><br><i><br></i></h4>\n                                    </div>\n                                </li>\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            <b>Đang cập nhật</b><br><i><br></i></h4>\n                                    </div>\n                                </li>  \n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            <b>Đang cập nhật</b><br><i><br></i></h4>\n                                    </div>\n                                </li>                                 \n                            </ul>\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Người điều phối</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/alexis-pham.jpeg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Bà <b>Alexis Phạm</b><br><i>Giám đốc nhân sự<br>Home Credit Việt Nam</i></h4>\n                                    </div>\n                                </li>   \n                            </ul>                       \n                        </div>         \n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">10:55</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Lễ vinh danh&nbsp;</b><b>DOANH NGHIỆP TIÊU BIỂU CÓ NGUỒN NHÂN LỰC HẠNH PHÚC 2023 - KHỐI DOANH NGHIỆP VỪA</b><br></h5> \n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">11:05</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Lễ vinh danh 100 NƠI LÀM VIỆC TỐT NHẤT VIỆT NAM 2023® - KHỐI DOANH NGHIỆP VỪA</b><b><br></b></h5> \n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">12:00</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <b>Ăn trưa &amp; Networking</b>\n                        </h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">13:30</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Phiên chiều - Đón khách &amp; Networking</b>\n                        </h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">13:35</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>TimeLapse - Báo cáo chuyên sâu độc quyền từ Khảo sát NƠI LÀM VIỆC TỐT NHẤT VIỆT NAM® 2023\n</b><div><b>BUILDING A RESILIENT TOMORROW – KIÊN HOẠT TIẾN BƯỚC&nbsp;</b></div></h5>\n                          <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                            Trong bối cảnh đầy thay đổi và biến động, ‘năng lực kiên cường phục hồi &amp; linh hoạt thích ứng’ – Resilience chính là điều kiện tiên quyết để một tổ chức tồn tại và phát triển bền vững. Tái định nghĩa Resilience? Yếu tố nào thực sự thúc đẩy năng lực Resilience của tổ chức &amp; nguồn nhân lực? Cùng khám phá các xu hướng &amp; thực hành mới nhất từ khảo sát độc quyền của Anphabe.</h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/thanh-nguyen.jpg\" alt=\"Thanh Nguyễn\">\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Bà <b>THANH NGUYỄN</b><br><i>GĐĐH &amp; Truyền cảm hứng Hạnh phúc<br>Anphabe</i></h4>\n                                    </div>\n                                </li>\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/vanluu-thumb.jpg\" alt=\"Vân Lưu\">\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Bà <b>VÂN LƯU</b><br><i>Giám đốc nghiên cứu thị trường<br>Intage Việt Nam</i></h4>\n                                    </div>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n                \n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">14:50</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>TimeFreeze: 5 NGUYÊN TẮC LÃNH ĐẠO AGILE</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                            Đặc sản của sự kiện Anphabe với các chia sẻ súc tích nhưng đầy cảm hứng từ các chuyên gia hàng đầu.\n                        </h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/prof-claire-macken-vertical.jpeg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Giáo sư <b>CLAIRE MACKEN</b><br><i>Tổng Giám Đốc<br>RMIT Việt Nam</i></h4>\n                                    </div>\n                                </li>\n                                \n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">15:20</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <b>Tiệc trà &amp; Networking</b>\n                        </h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">15:40</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>AnphaTalk</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\"></h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">\n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\"><b><i>Đang cập nhật</i></b><br><br></h4>\n                                    </div>\n                                </li>\n                                \n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">15:55</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>SpaceTime:  XUYÊN MƠ HỒ, LÁI THAY ĐỔI</b></h5>\n                        <h4 class=\"icon-title mbr-black mbr-fonts-style mb-4 display-7\">\n                          Những kinh nghiệm đa dạng, những bài học khác biệt, những bí quyết mang dấu ấn cá nhân của các lãnh đạo dày dạn đến từ những tổ chức đã &amp; đang từng bước “Ngược gió vươn cao”!                        </h4>\n                        <div class=\"schedule-speakers mb-4\">\n                            <div class=\"speaker-title\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                    <b>Diễn giả</b></p>\n                            </div>\n                            <ul class=\"speaker-list\">                              \n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/ms.-nguyn-th-thu-ha-adp-group.jpeg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Bà <b>NGUYỄN THỊ THU HÒA</b><br><i>Tổng Giám Đốc<br>ADP Group</i></h4>\n                                    </div>\n                                </li>  \n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/linkedin-rohit-kalsy.jpeg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            Ông <b>ROHIT KALSY</b><br><i>LinkedIn Regional Head</i><br><i>LinkedIn</i></h4>\n                                    </div>\n                                </li>  \n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/trithong-thumb.jpg\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">Ông <b>LÊ TRÍ THÔNGt</b><br><i>Phó Chủ Tịch HĐQT/Tổng Giám Đốc\n</i><div><i>PNJ Group</i></div></h4>\n                                    </div>\n                                </li>  \n                                <li>\n                                    <div class=\"speaker-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/profile.png\" alt>\n                                    </div>\n                                    <div class=\"speaker-detail\">\n                                        <h4 class=\"icon-title mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                            <b>Đang cập nhật</b><br><i><br></i></h4>\n                                    </div>\n                                </li> \n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">16:40</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Đêm vinh danh - Đón khách &amp; Networking</b></h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">17:40</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <b>Khai mạc lễ vinh danh</b>\n                        </h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">17:50</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\">\n                            <b>Lễ vinh danh THƯƠNG HIỆU NHÀ TUYỂN DỤNG HẤP DẤN VỚI SINH VIÊN VIỆT NAM 2023</b>\n                        </h5>\n                    </div>\n                </div>\n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">18:10</div>\n                    </div>\n                    <div class=\"text-box\">                        \n                        <h5 class=\"icon-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".icon-text\"><b>Lễ vinh danh DOANH NGHIỆP TIÊU BIỂU CÓ NGUỒN NHÂN LỰC HẠNH PHÚC 2023 - KHỐI DOANH NGHIỆP LỚN</b></h5>\n                    </div>\n                </div>                \n\n                <div class=\"item mbr-flex  item-border align-items-center\">\n                    <div class=\"time-wrapper\">\n                        <div class=\"mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7 time-slot\">18:30</div>\n                    </div>\n                    <div class=\"text-box\">